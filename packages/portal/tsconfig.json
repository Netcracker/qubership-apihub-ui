{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "paths": {
      "@apihub/components/*": [
        "./src/components/*"
      ],
      "@apihub/entities/*": [
        "./src/entities/*"
      ],
      "@apihub/api-hooks/*": [
        "./src/api-hooks/*"
      ],
      "@apihub/routes/*": [
        "./src/routes/*"
      ],
      "@apihub/utils/*": [
        "./src/utils/*"
      ],
      "@netcracker/qubership-apihub-ui-shared": [
        "./../shared/src"
      ],
      "@netcracker/qubership-apihub-ui-shared/*": [
        "./../shared/src/*"
      ]
    }
  },
  "include": [
    "src"
  ]
}
