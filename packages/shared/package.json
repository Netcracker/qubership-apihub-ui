{
  "name": "@netcracker/qubership-apihub-ui-shared",
  "version": "0.0.0",
  "private": true,
  "license": "Apache-2.0",
  "files": [
    "**/*"
  ],
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx,.json --ignore-path ../../.gitignore --max-warnings 0 --report-unused-disable-directives",
    "format": "npm run lint --fix",
    "storybook": "storybook dev -p 9009",
    "build:showcase": "storybook build -o ../../dist-showcase --disable-telemetry",
    "screenshot-test:server": "npm run build:showcase && ws -p 9009 -d ../../dist-showcase --spa index.html --static.maxage 3600",
    "screenshot-test": "npx playwright test -c src/it/playwright --project=chromium",
    "screenshot-test:ci": "start-server-and-test screenshot-test:server http://localhost:9009 screenshot-test",
    "screenshot-test:docker": "start-server-and-test screenshot-test:server http://localhost:9009 \"bash ./src/it/playwright/run-docker-test.sh\"",
    "screenshot-test:regenerate-snapshots": "start-server-and-test screenshot-test:server http://localhost:9009 \"bash ./src/it/playwright/run-docker-test.sh --update-snapshots\""
  },
  "peerDependencies": {
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@mui/icons-material": "5.11.9",
    "@mui/material": "5.11.10",
    "@mui/private-theming": "5.11.9",
    "js-file-download": "0.4.12",
    "jszip": "3.10.1"
  },
  "devDependencies": {
    "@playwright/test": "1.51.0",
    "@types/node": "22.13.10",
    "vite-plugin-dts": "1.7.1"
  }
}
